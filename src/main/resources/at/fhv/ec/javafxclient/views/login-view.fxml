<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<HBox minHeight="600.0" minWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.fhv.ec.javafxclient.view.LoginController">
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0">
         <children>
            <TextField onAction="#onLoginButtonClicked" fx:id="usernameTextField" layoutX="315.0" layoutY="225.0" />
            <Text layoutX="235.0" layoutY="242.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
            <Text layoutX="237.0" layoutY="293.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
            <Button layoutX="369.0" layoutY="326.0" mnemonicParsing="false" onAction="#onLoginButtonClicked" text="Login" />
            <Label fx:id="statusLabel" layoutX="378.0" layoutY="369.0" />
            <PasswordField onAction="#onLoginButtonClicked" fx:id="passwordTextField" layoutX="311.0" layoutY="276.0" />
         </children>
      </Pane>
   </children>
</HBox>
